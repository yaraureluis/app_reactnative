{
  "expo": {
    "name": "Patilla Wish List",
    "slug": "Patilla",
    "privacy": "public",
    "description": "An application to list your wishes",
    "version": "1.0.0",
    "platforms": ["android"],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F48596"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.yaraure.patilla",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.yaraure.patilla",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F48596"
      },
      "permissions": ["CAMERA", "ACCESS_FINE_LOCATION", "WRITE_EXTERNAL_STORAGE"],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCzIwMwGpVGresrGyKmLX7Bi1zL-NvOMVg"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
